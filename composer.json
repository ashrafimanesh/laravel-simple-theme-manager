{
    "name": "ashrafi/laravel-simple-theme-manager",
    "description": "Laravel Theme Manager",
    "license": "MIT",
    "authors": [
        {
            "name": "ramin",
            "email": "ashrafimanesh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ashrafi\\ThemeManager\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ashrafi\\ThemeManager\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
